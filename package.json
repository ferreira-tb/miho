{
  "name": "miho",
  "version": "5.1.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.4.0",
  "scripts": {
    "clippy": "cargo clippy",
    "format": "cargo fmt --all",
    "mock:bump": "cargo run -- bump -t -p mocks",
    "mock:update": "cargo run -- update -t -p mocks",
    "release": "pnpm run clippy && cargo publish -p miho_derive && cargo publish miho"
  }
}