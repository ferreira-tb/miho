{
  "name": "miho",
  "version": "1.7.3",
  "description": "Easily bump your package version",
  "license": "MIT",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@8.11.0",
  "homepage": "https://tb.dev.br/miho/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferreira-tb/miho.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/miho/issues"
  },
  "keywords": [
    "miho",
    "version",
    "bump",
    "semver",
    "package"
  ],
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json}": "prettier --write"
  },
  "scripts": {
    "build": "node --loader ts-node/esm scripts/build.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs && pnpm docs:typedoc",
    "docs:preview": "vitepress preview docs",
    "docs:typedoc": "pnpm exec typedoc --plugin typedoc-plugin-mdn-links",
    "format": "pnpm exec prettier . --write",
    "format-check": "pnpm exec prettier . --check",
    "minify": "pnpm exec minify-it dist",
    "prepare": "husky install",
    "release": "miho -l publish",
    "rollup": "rollup --config rollup.config.ts --configPlugin typescript",
    "taze": "pnpm exec taze -w -f",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui --watch --coverage.enabled=true",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "c12": "^1.5.1",
    "chalk": "^5.3.0",
    "detect-indent": "^7.0.1",
    "execa": "^8.0.1",
    "glob": "^10.3.10",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@octokit/core": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.6",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-istanbul": "1.0.0-beta.5",
    "@vitest/ui": "1.0.0-beta.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "minify-it": "^1.1.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.0",
    "rollup": "^4.5.2",
    "taze": "^0.12.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.3",
    "typedoc-plugin-mdn-links": "^3.1.4",
    "typescript": "5.2.2",
    "unocss": "^0.57.7",
    "vite": "^5.0.2",
    "vite-plugin-dts": "^3.6.3",
    "vitepress": "1.0.0-rc.31",
    "vitest": "1.0.0-beta.5",
    "vue": "^3.3.8"
  },
  "bin": {
    "miho": "bin/miho.mjs"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  }
}